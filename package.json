{
  "author": "Aleksei Arro",
  "browserslist": [
    "last 5 versions",
    "ie >= 11"
  ],
  "description": "Here I practice in making of advanced Gulp.js configuration",
  "devDependencies": {
    "autoprefixer": "9.7.3",
    "cross-env": "6.0.3",
    "del": "5.1.0",
    "gulp": "4.0.2",
    "gulp-debug": "4.0.0",
    "gulp-if": "3.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-stylus": "2.7.0"
  },
  "keywords": [
    "gulp",
    "node"
  ],
  "license": "ISC",
  "main": "gulpfile.js",
  "name": "learn-gulp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yialo/learn-gulp.git"
  },
  "scripts": {
    "assemble": "gulp assemble --gulpfile ./gulp/index.js --cwd ./",
    "copy": "gulp copy --gulpfile ./gulp/index.js --cwd ./",
    "styles": "gulp styles --gulpfile ./gulp/index.js --cwd ./",
    "launch": "npm run assemble",
    "dev": "cross-env NODE_ENV=development npm run launch",
    "build": "cross-env NODE_ENV=production npm run launch"
  },
  "version": "1.0.0"
}
